import java.util.Scanner;

public class Principal {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Introduce un número");
		int num = sc.nextInt();
		
		System.out.println(num);
		num=num+1;
		System.out.println(num);
		num=num+1;
		System.out.println(num);
		num=num+1;
		System.out.println(num);
		num=num+1;
		System.out.println(num);
		
		
	}

}
